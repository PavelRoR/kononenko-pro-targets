#get
	padding 0 0 60px
.get-list
	// padding 20px
	width 100%
	max-width 860px
	margin 0 auto
	// background-color: rgba(255,255,255,.25)
	li
		font-size: 20px
		margin-bottom: 15px
		position relative
		padding-left: 17px
		list-style: none
		&:last-child
			margin-bottom: 0
		&:after
			content: ''
			display block
			width 6px
			height 6px
			position: absolute
			top 11px
			left 2px
			background-color: rgb(224,26,16)
			border-radius: 50px
			border: 1px solid #fff
		&:before
			content: ''
			display block
			width 10px
			height 10px
			position: absolute
			top 9px
			left 0
			background-color: rgb(224,26,16)
			border-radius: 50px